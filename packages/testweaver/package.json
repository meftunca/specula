{
  "name": "@testweaver/cli",
  "version": "0.1.0",
  "description": "Context-based automatic test generation CLI tool for React/Vue/Svelte/HTML projects",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "testweaver": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "node --import tsx src/cli/index.ts generate",
    "dev:watch": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cli": "npm run build && node dist/cli/index.js generate",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "test:parser": "node --import tsx src/test-parser.ts",
    "generate:samples": "node --import tsx src/generate-samples.ts"
  },
  "keywords": [
    "test",
    "generator",
    "react",
    "vue",
    "svelte",
    "cli",
    "typescript",
    "testing",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/babel__traverse": "^7.28.0",
    "@types/node": "^20.10.0",
    "@types/react": "^19.2.7",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "tsx": "^4.20.6",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "chokidar": "^4.0.3",
    "commander": "^11.1.0",
    "cosmiconfig": "^9.0.0",
    "glob": "^11.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist"
  ]
}
